{
  "name": "Personal Assistant Streaming",
  "description": "Agent test with streaming",
  "setup": {
    "provider_type": "multi",
    "provider_config": {
      "openai_api_key": "test-key",
      "openai_use_responses": [false, true]
    }
  },
  "input": {
    "model_name": ["gpt-4.1", "openai/gpt-4.1"],
    "run_agent": true,
    "stream": true,
    "user_input": "What's 2+2?"
  },
  "expected": {
    "agent_output": {
      "contains_text": ["4"],
      "min_length": 1
    }
  }
}

