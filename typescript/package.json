{
  "name": "@timestep-ai/timestep",
  "version": "2026.0.52",
  "description": "Timestep AI - Durable agent execution framework with cross-language state persistence and multi-model support for OpenAI Agents",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "test": "vitest run --bail=1",
    "test:watch": "vitest",
    "visualize": "tsx timestep/cli/visualize.ts"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@10.17.1",
  "pnpm": {
    "overrides": {
      "@openai/agents": "link:../3rdparty/openai-agents-js/packages/agents",
      "@openai/agents-core": "link:../3rdparty/openai-agents-js/packages/agents-core",
      "@openai/agents-openai": "link:../3rdparty/openai-agents-js/packages/agents-openai"
    }
  },
  "dependencies": {
    "@mendable/firecrawl-js": "^1.0.0",
    "@modelcontextprotocol/sdk": "^1.24.0",
    "debug": "^4.4.0",
    "ms": "^2.1.3",
    "ollama": "^0.6.3",
    "openai": "^6.0.0",
    "postgres": "^3.4.7",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@openai/agents": "link:../3rdparty/openai-agents-js/packages/agents",
    "@openai/agents-core": "link:../3rdparty/openai-agents-js/packages/agents-core",
    "@openai/agents-openai": "link:../3rdparty/openai-agents-js/packages/agents-openai",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "pg": "^8.11.3",
    "testcontainers": "^10.0.0",
    "tsup": "^8.5.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "zod": "^4.1.13"
  }
}
