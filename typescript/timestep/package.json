{
	"name": "@timestep-ai/timestep",
	"version": "2025.09.171016",
	"description": "A CLI tool built with Ink and React",
	"keywords": [
		"cli",
		"ink",
		"react",
		"terminal",
		"interactive"
	],
	"author": "Your Name <your.email@example.com>",
	"homepage": "https://github.com/yourusername/timestep#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/yourusername/timestep.git"
	},
	"bugs": {
		"url": "https://github.com/yourusername/timestep/issues"
	},
	"license": "MIT",
	"main": "dist/index.js",
	"bin": "dist/cli.js",
	"type": "module",
	"exports": {
		".": "./dist/index.js",
		"./server": "./dist/server.js"
	},
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava",
		"prepublishOnly": "npm run build",
		"server": "node dist/server.js"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@a2a-js/sdk": "^0.3.4",
		"@modelcontextprotocol/sdk": "^1.18.0",
		"@openai/agents": "^0.1.2",
		"@openai/agents-core": "^0.1.2",
		"@types/express": "^5.0.3",
		"@types/uuid": "^10.0.0",
		"express": "^5.1.0",
		"ink": "^4.1.0",
		"meow": "^11.0.0",
		"ollama": "^0.5.17",
		"openai": "^5.20.2",
		"react": "^18.2.0",
		"uuid": "^13.0.0",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.53.1"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
