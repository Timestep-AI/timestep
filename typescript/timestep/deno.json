{
	"imports": {
		"oak": "https://deno.land/x/oak@v12.6.1/mod.ts",
		"uuid": "https://deno.land/std@0.200.0/uuid/mod.ts",
		"express": "npm:express@^4.21.2",
		"@modelcontextprotocol/sdk/": "npm:@modelcontextprotocol/sdk/",
		"@a2a-js/sdk": "npm:@a2a-js/sdk",
		"@a2a-js/sdk/server": "npm:@a2a-js/sdk/server"
	},
	"nodeModulesDir": "auto",
	"compilerOptions": {
		"lib": ["deno.window", "dom"],
		"strict": true
	},
	"tasks": {
		"start": "deno run --allow-net --allow-read --allow-write --allow-env src/index.ts",
		"dev": "deno run --allow-net --allow-read --allow-write --allow-env --watch src/index.ts",
		"add:express": "deno add npm:express",
		"deno-server": "deno run --allow-net --allow-read --allow-write --allow-env --allow-sys src/denoServer.ts",
		"deno-server:dev": "deno run --allow-net --allow-read --allow-write --allow-env --allow-sys --watch src/denoServer.ts",
		"server": "deno run --allow-net --allow-read --allow-write --allow-env --allow-sys src/server.ts",
		"server:dev": "deno run --allow-net --allow-read --allow-write --allow-env --allow-sys --watch src/server.ts"
	}
}
